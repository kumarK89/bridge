    "whatwg-fetch": "1.0.0"
  },
  "dependencies": {
    "axios": "^0.15.1",
    "i18nliner": "^0.1.6",
    "jquery": "^3.1.1",
    "raven-js": "^3.7.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-modal": "^1.5.2",
    "react-router": "^2.8.1"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "nyc mocha ./test/**/*Spec.js",
    "build-css": "node-sass --importer ./node_modules/node-sass-json-importer/dist/node-sass-json-importer.js --include-path scss ./src/assets/stylesheets/styleguide.scss --output ./src/assets/stylesheets/",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "dev": "npm run build-css && npm run start",
    "qa": "npm run build-css && npm run test",
    "prod": "npm run build-css && npm run build",
    "build-icon-css": "node-sass --include-path scss ./src/assets/stylesheets/shared/_icons.scss --output ./src/assets/stylesheets/"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "node_modules/",
      "assets/"
    ],
    "extension": [
      ".js",
      ".jsx"
    ],
    "require": [
      "babel-register",
      "./test/helpers/browser.js"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true,
    "all": true
  },
  "babel": {
    "presets": [
      "es2015","react"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
